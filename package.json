{
  "name": "@wyze/koa-file-server",
  "description": "file serving middleware for koa",
  "version": "3.2.2",
  "author": {
    "name": "Jonathan Ong",
    "email": "me@jongleberry.com",
    "url": "http://jongleberry.com",
    "twitter": "https://twitter.com/jongleberry"
  },
  "repository": "wyze/file-server",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wyze/spdy-push": "^2.0.1",
    "compressible": "^2.0.9",
    "debug": "*",
    "hash-stream": "^1.2.0",
    "mime-types": "^2.1.13",
    "mz": "^2.6.0",
    "resolve-path": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "fast-async": "^6.1.2",
    "istanbul-harmony": "^0.3.16",
    "koa": "^2.0.0-alpha.7",
    "mocha": "^3.2.0",
    "should": "^11.1.1",
    "spdy": "^3.4.4",
    "spdy-keys": "*",
    "supertest": "^2.0.1"
  },
  "scripts": {
    "preversion": "babel index.js --out-dir lib",
    "test": "NODE_ENV=test mocha --harmony --require should --reporter spec --bail",
    "test-cov": "node node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --harmony --reporter dot --require should",
    "test-travis": "node node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --harmony --reporter dot --require should"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "koa",
    "file",
    "server",
    "static"
  ],
  "babel": {
    "plugins": [
      "fast-async"
    ]
  }
}
